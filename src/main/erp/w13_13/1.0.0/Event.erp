enum EventType{
  case Birthday
  case BirthdayParty
  case BBQ
  case Excursion
  case MindBody
  case Education
}
enum Frequency{
  case Oneoff
  case Weekly
  case Fortnightly
  case Monthly
  case Sixmonthly
  case Yearly
}
entity Event{
  field title : String
  field details : [Text]
  field eventType : [EventType]
  field people: [Person]
  field date : DateTime
  field location : String
  field frequency : [Frequency]
}
                   