extend access Salesperson {
  entity SalesOrder = readWrite
  entity SalesOrderItem = readWrite
  entity SalesDelivery = readWrite
  entity SalesOrderDelivery = readWrite
  entity SalesDeliveryItem = readWrite
  entity IncomePayment = readWrite
  entity SalesInvoiceIncomePayment = readWrite
  menu Sales {
    entity SalesOrder
    entity SalesDelivery
    entity SalesInvoice
    entity IncomePayment
    trait ISuperProduct
  }
}