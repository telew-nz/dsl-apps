entity MarketingCentre
  
{  field manualTitle : String
   field userInputs : UserInput
   field details : String
   field comments : String 
   field backlogCategory: BacklogCategory 
   field status : Status
   field usecase : UseCase
   field contact : People
   field relatedMarketing : MarketingCentre
   field category : [MarketingCategory]
  
     
  compute title = manualTitle
}