entity Job{
  field title : String
  field details : Text
  field company : Company
  field assigned : Person
  field jobType : JobType
  field locationRequirement: LocationRequirement
}
  
 enum  JobType{
   case Freelance
   case Employment
   case LongTermFlexi
   case LongTermCore
  }
     
 enum LocationRequirement{
   case LocalFlexi
   case LocalFixedTerm
   case Virtual
   }
   
    